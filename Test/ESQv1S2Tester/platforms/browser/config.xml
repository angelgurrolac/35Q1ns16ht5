<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eisei.esqinsights" version="1.0.0" xmlns="http://www.esq.com/" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>ESQ Insights</name>
    <description>
        First aplication demo of ESQ Insights.
    </description>
    <author email="angel.gurrola@playful.mx" href="http://eisei.mx">
        Angel Gurrola angel.gurrola@playful.mx
        Julieta Gonzalez julieta.gonzalez@playful.mx
        Jose Estrada jose.estrada@playful.mx
        Sheyla Rodriguez sheyla.rodriguez@playful.mx
        Jose Martinez jose.martinez@playful.mx
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
